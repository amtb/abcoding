    "@types/jest": "^25.2.1",
    "babel-jest": "^25.5.1",
    "colors": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.2",
    "lodash": "^4.17.21",
    "mustache": "^4.1.0",
    "prettier": "2.0.5",
    "simple-git": "^2.15.0",
    "yargs": "^15.3.1"
  },
  "husky": {
